{
  "name": "aimux",
  "version": "2.0.0-rc3",
  "description": "Multi-provider CLI tool for Claude Code with intelligent routing, failover, and provider management",
  "main": "dist/index.js",
  "bin": {
    "aimux": "dist/cli/index.js"
  },
  "scripts": {
    "build": "tsc",
    "build:tsc": "tsc",
    "build:router": "echo 'Router build: TypeScript implementation (MVP)' && npm run build",
    "build:native": "echo 'Native C++ router: Not implemented in MVP' && echo 'See native/README.md for future implementation'",
    "dev": "ts-node src/index.ts",
    "dev:router": "echo 'Router dev mode: TypeScript implementation (MVP)' && npm run dev",
    "start": "node dist/index.js",
    "start:router": "echo 'Starting router only... (MVP)' && echo 'Use: npm run dev && aimux router start'",
    "test": "jest",
    "test:router": "jest --testNamePattern='router'",
    "test:integration": "jest --testNamePattern='integration'",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "format": "prettier --write \"src/**/*.ts\"",
    "prepare": "npm run build",
    "prepublishOnly": "npm test",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags",
    "router:info": "echo '=== Aimux Router Information ===' && echo 'Version: MVP (TypeScript-based)' && echo 'Future: Native C++ implementation' && echo 'Files: src/router/' && echo 'Docs: native/README.md'"
  },
  "keywords": [
    "claude",
    "anthropic",
    "synthetic",
    "ai",
    "cli",
    "model-selection",
    "typescript",
    "nodejs",
    "multi-provider",
    "router",
    "failover"
  ],
  "author": "Aimux Contributors",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/parnexcodes/aimux"
  },
  "bugs": {
    "url": "https://github.com/parnexcodes/aimux/issues"
  },
  "homepage": "https://github.com/parnexcodes/aimux#readme",
  "dependencies": {
    "axios": "^1.6.2",
    "chalk": "^5.3.0",
    "commander": "^11.1.0",
    "ink": "^3.2.0",
    "zod": "^3.22.4",
    "react": "^18.2.0"
  },
  "devDependencies": {
    "@types/jest": "^29.5.8",
    "@types/node": "^20.9.0",
    "@types/react": "^18.2.37",
    "@typescript-eslint/eslint-plugin": "^6.11.0",
    "@typescript-eslint/parser": "^6.11.0",
    "eslint": "^8.54.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.1",
    "jest": "^29.7.0",
    "prettier": "^3.1.0",
    "react-devtools-core": "^4.28.5",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2"
  },
  "files": [
    "dist/",
    "README.md",
    "LICENSE",
    "scripts/"
  ],
  "engines": {
    "node": ">=18.0.0"
  }
}